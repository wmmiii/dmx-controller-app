{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    {
      "identifier": "fs:default",
      "allow": [{ "path": "$APPDATA/*" }]
    },
    {
      "identifier": "fs:read-files",
      "allow": [{ "path": "$APPDATA/*" }]
    },
    {
      "identifier": "fs:write-files",
      "allow": [{ "path": "$APPDATA/*" }]
    }
  ]
}
