{
  "search.exclude": {
    "bazel-*": true
  },
  "files.exclude": {
    "bazel-*": true
  },
  "files.watcherExclude": {
    "bazel-*": true
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "cSpell.words": [
    "blobstore",
    "bufbuild",
    "gdtf",
    "lerp",
    "prost",
    "sacn",
    "Tauri",
    "wavesurfer",
    "wled"
  ],
  "[proto]": {
    "editor.defaultFormatter": "xaver.clang-format"
  }
}
